<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="320.0" prefWidth="480.0">
         <children>
            <Label layoutX="59.0" layoutY="84.0" prefHeight="20.0" prefWidth="150.0" text="Server Receving Port">
               <effect>
                  <Glow>
                     <input>
                        <Blend />
                     </input>
                  </Glow>
               </effect>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="59.0" layoutY="160.0" prefHeight="20.0" prefWidth="131.0" text="Server Sending Port">
               <effect>
                  <Glow>
                     <input>
                        <Blend />
                     </input>
                  </Glow>
               </effect>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <TextArea fx:id="recPortField" blendMode="MULTIPLY" depthTest="DISABLE" layoutX="59.0" layoutY="104.0" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="128.0" />
            <TextArea fx:id="sendPortField" blendMode="MULTIPLY" depthTest="DISABLE" layoutX="59.0" layoutY="180.0" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="128.0" />
            <Button fx:id="startButton" layoutX="146.0" layoutY="235.0" mnemonicParsing="false" onMouseClicked="#onClickStartButton" text="Start" />
            <ImageView fx:id="logoContainer" fitHeight="200.0" fitWidth="200.0" layoutX="240.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
            	
      		</ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
