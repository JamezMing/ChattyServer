                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="407.0" prefWidth="419.0">
                     <children>
                        <TextArea fx:id="messageDisplayArea" blendMode="MULTIPLY" depthTest="DISABLE" editable="false" minHeight="30.0" prefHeight="407.0" prefWidth="419.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
